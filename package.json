{
  "name": "graphql-upload-minimal",
  "version": "0.1.0",
  "description": "Minimalistic and developer friendly middleware and an Upload scalar to add support for GraphQL multipart requests (file uploads via queries and mutations) to various Node.js GraphQL servers.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:koresar/graphql-upload-minimal",
  "homepage": "https://github.com/koresar/graphql-upload-minimal#readme",
  "bugs": "https://github.com/koresar/graphql-upload-minimal/issues",
  "keywords": [
    "graphql",
    "upload",
    "file",
    "multipart",
    "server",
    "koa",
    "express",
    "apollo",
    "esm",
    "mjs"
  ],
  "files": [
    "private",
    "public"
  ],
  "main": "public",
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/": "./public/",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^10.13.0 || ^12.0.0 || >= 13.7.0"
  },
  "peerDependencies": {
    "graphql": "0.13.1 - 15"
  },
  "dependencies": {
    "busboy": "^0.3.1"
  },
  "devDependencies": {
    "coverage-node": "^2.0.3",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "graphql": "^15.0.0",
    "jsdoc-md": "^6.0.0",
    "koa": "^2.12.0",
    "mocha": "^7.2.0",
    "node-fetch": "^2.6.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc",
    "prepare:jsdoc": "jsdoc-md",
    "ci": "npm run lint && npm t",
    "test": "mocha test --recursive",
    "lint": "eslint .",
    "cov": "nyc npm run test",
    "prepublishOnly": "npm test"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 11
    },
    "rules": {}
  }
}
